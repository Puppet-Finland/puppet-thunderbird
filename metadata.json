{
  "name": "puppetfinland-thunderbird",
  "version": "1.0.1",
  "author": "Puppet-Finland team",
  "summary": "Install Mozilla Thunderbird and configure email accounts for users",
  "license": "BSD-2-Clause",
  "source": "https://github.com/Puppet-Finland/puppet-thunderbird.git",
  "project_page": "https://github.com/Puppet-Finland/puppet-thunderbird",
  "dependencies": [
    {
      "name": "rismoney/chocolatey",
      "version_requirement": ">= 0.4.1"
    },
    {
      "name": "puppetlabs/concat",
      "version_requirement": ">= 1.1.2"
    },
    {
      "name": "puppetfinland/os",
      "version_requirement": ">= 0.5.7"
    },
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8",
        "9"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "24"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04"
      ]
    },
    {
      "operatingsystem": "windows"
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.7.0 < 6.0.0"
    }
  ],
  "pdk-version": "1.4.1",
  "template-url": "file:///opt/puppetlabs/pdk/share/cache/pdk-templates.git",
  "template-ref": "1.4.1-0-g52adbbb"
